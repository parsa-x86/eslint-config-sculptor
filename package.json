{
  "name": "eslint-config-sculptor",
  "version": "0.1.0",
  "description": "A flexible ESLint config with optional rule sets. Enable only what you need!",
  "keywords": [
    "eslint",
    "lint"
  ],
  "author": "parsa-x86",
  "license": "MIT",
  "packageManager": "pnpm@10.8.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/parsa-x86/eslint-config-sculptor.git"
  },
  "bugs": {
    "url": "https://github.com/parsa-x86/eslint-config-sculptor/issues"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "require": {
        "types": "./index.d.cts",
        "default": "./index.cjs"
      },
      "default": {
        "types": "./index.d.ts",
        "default": "./index.js"
      }
    }
  },
  "type": "module",
  "scripts": {
    "build": "tsup",
    "release": "release-it",
    "postbuild": "cp package.json dist/"
  },
  "devDependencies": {
    "release-it": "^19.0.1",
    "tsup": "^8.4.0",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "globals": "^16.0.0",
    "typescript-eslint": "^8.31.0"
  },
  "peerDependencies": {
    "eslint": "^9.0.0",
    "typescript": "^5.0.0"
  }
}
